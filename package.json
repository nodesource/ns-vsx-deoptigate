{
  "name": "ns-vsx-deoptigate",
  "displayName": "ns-vsx-deoptigate",
  "description": "Adds deoptigate information to your project which is updated when project executes.",
  "version": "0.0.1",
  "publisher": "nodesource",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [],
    "debuggers": [
      {
        "type": "NodeSource",
        "request": "launch",
        "label": "NodeSource: deoptigate",
        "runtime": "node",
        "languages": [
          "javascript",
          "typescript",
          "javascriptreact",
          "typescriptreact"
        ],
        "configurationSnippets": [
          {
            "type": "node",
            "label": "NodeSource: deoptigate",
            "description": "Run app and generate deoptigate data to see optimization feedback in editor.",
            "body": {
              "type": "node",
              "request": "launch",
              "name": "NS|deoptigate:${2}",
              "program": "^\"\\${workspaceFolder}/${1:app.js}\"",
              "runtimeArgs": [
                "--trace-ic",
                "^\"--logfile=\\${workspaceFolder}/.vscode/deoptigate.log\"",
                "--nologfile-per-isolate"
              ]
            }
          }
        ],
        "initialConfigurations": [
          {
            "type": "node",
            "request": "launch",
            "name": "NS|deoptigate:app",
            "program": "${workspaceFolder}/app.js",
            "runtimeArgs": [
              "--trace-ic",
              "--logfile=${workspaceFolder}/.vscode/deoptigate.log",
              "--nologfile-per-isolate"
            ]
          }
        ]
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "deoptigate": "~0.4.2",
    "opn": "~5.4.0"
  }
}
